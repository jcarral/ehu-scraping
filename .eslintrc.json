{
	"env": {
		"browser": true,
		"node": true
	},
	"extends": "airbnb",
	"rules": {
		"react/jsx-filename-extension": 0,
    "func-names": 0,
    "no-underscore-dangle": 0,
    "no-prototype-builtins": 0,
    "prefer-promise-reject-errors": 0,
    "prefer-destructuring": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "quote-props": 0,
    "no-throw-literal": 0,
    "no-restricted-globals": 0,
    "no-plusplus": 0
	}
}
